<div class="banner">
    <div class="w1000 ui-location-reporting">
        <div class="toggle-container-box">
            <div class="toggle-title">
                <span class="active">目标定位</span>
                <span id="intelligent"><a target="_blank" href="/predict-volunteer.html">智能填报</a></span>
            </div>
            <div class="toggle-content ui-location">
                <label for="subject-mw">文史&nbsp;&nbsp;<input type="radio" name="mwl" id="subject-mw" value="1"></label>
                <label for="subject-ml">理工&nbsp;&nbsp;<input type="radio" name="mwl" id="subject-ml"value="2"></label>
                <input type="text" class="input-common" id="target-score" placeholder="输入分数">
                <input type="text" class="input-common" zgk-type="autoComplete" zgk-api="U" id="target-school" placeholder="目标院校">
                <button class="btn btn-success btn-start" id="report-btn">开始定位</button>
            </div>
            <div id="tips" class="alert alert-danger" role="alert"></div>
        </div>
    </div>
</div>
