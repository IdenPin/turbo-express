</div>
<!--高考头条-->
<div class="gk-headlines w1000">
    <h2 class="h2-t w1000"><span class="line"></span>高考头条 <a href="/news-hot.html" class="more-btn">更多</a></h2>
    <div class="headlines-list" id="gk-top">
        {{#each rows}}
            <div class="box">
                <a href="/news-detail.html?hid={{id}}" class="img-cover" target="_blank">
                    <img src="{{image}}@base@tag=imgScale&q=30" alt="{{title}}">
                </a>
                <h3><a href="/news-detail.html?hid={{id}}" target="_blank">{{title}}</a></h3>
                {{#if subContent}}
